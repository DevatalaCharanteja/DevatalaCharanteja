


#include <stdio.h>
int main(void) 
{
    int i,j;
    int n;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        printf("%d\t",i);
    }
    printf("\n");
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            printf("%d\t",i*j);
        }
    
    
        printf("\n");
}

    return 0;
}
