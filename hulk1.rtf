#include <stdio.h>
#include <stdlib.h>
 
int main()
{
  charan(3, 'A', 'B', 'C');
}
 
void charan(int n, char str, char Middle, char rodTo)
{

    if(n==1)
    {
        printf("MOVING RING %d FROM TO %c \n",n,str,rodTo);
        return;
    }
    charan(n-1,str,rodTo,Middle);
    printf("MOVING RING %d FROM %c TO %c \n",n,str,rodTo);
    charan(n-1,Middle,str,rodTo);
 
}
