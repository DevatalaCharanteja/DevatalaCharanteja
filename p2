n = int(input())                     
t = n                                
r = 0                                
while n > 0:                         
    d = n % 10                       
    r = r * 10 + d                   
    n = n // 10                      
if t == r:                           
    print('PALINDROME NUMBER')       
else:                                
    print('NOT A PALINDROME NUMBER') 

