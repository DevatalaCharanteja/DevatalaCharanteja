n = int(input())
num = int(input('Enter num:'))
largest = num  # 10
smallest = num
i = 1
while i < n:
    num = int(input('enter num:'))
    # 20
    if num > largest:
        largest = num
    if num < smallest:
        smallest = num
    i = i + 1
dif = largest - smallest
print(' largest num ', largest)
print('smallest num', smallest)
print('difference=', dif)


